<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %} | Koninklijk Belgische Voetbalbond{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('node_modules/pikaday/css/pikaday.css') }}">
            <link rel="stylesheet" href="{{ asset('css/style.css') }}">
        {% endblock %}
    </head>
    <body>
        {% block body %}
            
                <div class="background"></div>
                <div class="contents">
                    {% block contents %}{% endblock %}
                </div>
                {% include 'includes/footer.html.twig' %}

        {% endblock %}
        {% block javascripts %}
            <script src="{{ asset('node_modules/moment/moment.js') }}"></script>
            <script src="{{ asset('node_modules/pikaday/pikaday.js') }}"></script>
            <script src="{{ asset('js/main.js') }}"></script>
        {% endblock %}
    </body>
</html>
